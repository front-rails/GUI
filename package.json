{
  "name": "gui",
  "version": "0.0.0",
  "description": "Front End for Stack Undertow",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "browser-sync start --server 'dist'",
    "start:dev": "browser-sync start --server --directory --startPath='src/index.html'",
    "sass": "node-sass src/scss/main.scss dist/css/main.css",
    "sass:dev": "node-sass src/scss/main.scss src/css/main.css --watch='src/scss/*.scss'",
    "wiredep": "wiredep --src 'src/index.html,src/scss/main.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-rails/GUI.git"
  },
  "author": "Lacey Rice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/front-rails/GUI/issues"
  },
  "homepage": "https://github.com/front-rails/GUI#readme",
  "dependencies": {
    "bower": "^1.6.3",
    "browser-sync": "^2.9.11",
    "gulp": "^3.9.0",
    "node-sass": "^3.3.3",
    "wiredep": "^2.2.2"
  }
}
